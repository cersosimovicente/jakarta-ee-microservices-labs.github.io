{
  "name": "Java 25 & Maven Latest",
  "image": "mcr.microsoft.com",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "25",
      "installMaven": "true",
      "mavenVersion": "3.9.12"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack"
      ]
    }
  },
  "postCreateCommand": "mvn -version"
}
